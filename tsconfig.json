{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": [
        "esnext",
        "dom"
      ],
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "noEmit": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "incremental": true,
      "isolatedModules": true
    },
    "include": [
      "src",
    ],
  }
